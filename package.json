{
  "name": "full-fuel-api",
  "version": "1.0.0",
  "description": "Full Fuel API",
  "main": "index.ts",
  "scripts": {
    "clean": "rm -r ./build",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --quiet --fix",
    "build": "npm run lint && tsc",
    "test": "jest",
    "dev": "env-cmd -f ./src/config/dev.env nodemon -L"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "ddd"
  ],
  "author": "Pedro Medina",
  "license": "ISC",
  "dependencies": {
    "class-transformer": "^0.3.1",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "mysql2": "^2.1.0",
    "node-fetch": "^2.6.1",
    "sequelize": "5.22.3",
    "uuidv4": "^6.2.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.1",
    "@types/node-fetch": "^2.5.7",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "git-commit-msg-linter": "^2.4.5",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.13",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "reflect-metadata": "^0.1.13",
    "sequelize-typescript": "^1.1.0",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
