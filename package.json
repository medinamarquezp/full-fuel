{
  "name": "full-fuel-api",
  "version": "1.0.0",
  "description": "Full Fuel API",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --quiet --fix",
    "build": "npm run lint && tsc",
    "postbuild": "ef-tspm",
    "postinstall": "patch-package",
    "start": "node build/apps/api/app.js",
    "test": "jest --detectOpenHandles --forceExit --silent",
    "dev": "nodemon -L",
    "executeTasks": "node build/apps/scheduledTasks/scheduledTasks.js",
    "cacheJob": "node build/apps/scheduledTasks/jobs/CacheJob.js",
    "ccaaJob": "node build/apps/scheduledTasks/jobs/CCAAJob.js",
    "fuelStationsJob": "node build/apps/scheduledTasks/jobs/FuelStationsJob.js",
    "notificationsJob": "node build/apps/scheduledTasks/jobs/NotificationsJob.js",
    "removePricesJob": "node build/apps/scheduledTasks/jobs/RemovePricesJob.js"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "ddd"
  ],
  "author": "Pedro Medina",
  "license": "ISC",
  "dependencies": {
    "class-transformer": "^0.3.1",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "firebase-admin": "^9.3.0",
    "geolocation-utils": "^1.2.3",
    "helmet": "^4.1.1",
    "mysql2": "^2.2.5",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.7",
    "rate-limiter-flexible": "^2.1.13",
    "redis": "^3.1.1",
    "sequelize": "6.29.0",
    "uuidv4": "^6.2.4",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@types/express": "^4.17.8",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/node-cron": "^2.0.3",
    "@types/node-fetch": "^2.5.7",
    "@types/redis": "^2.8.28",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "git-commit-msg-linter": "^2.7.0",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.0",
    "nodemon": "^2.0.21",
    "patch-package": "^6.2.2",
    "prettier": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "sequelize-typescript": "^1.1.0",
    "ts-jest": "^26.4.3",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
